{
  "name": "pivovartsev/is-config-reader",
  "autoload": {
    "psr-4": {
      "StanislavPivovartsev\\InterestingStatistics\\ConfigReader\\": "src/",
      "StanislavPivovartsev\\InterestingStatistics\\ConfigReader\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "10.5.19",
    "phpstan/phpstan": "^1.11"
  },
  "scripts": {
    "phpstan": "vendor/bin/phpstan analyze src tests",
    "phpstan-clear-cache": "vendor/bin/phpstan clear-result-cache",
    "infection": "infection --show-mutations"
  }
}
